{
  "name": "react-with-electron",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.js",
  "productName": "React Electron App",
  "homepage": "./",
  "author": {
    "name": "React Electron, Inc.",
    "email": "support@react.com",
    "url": "http://reactelectron.com"
  },
  "build": {
    "appId": "com.react-electron.autoupdater",
    "productName": "React Electron App",
    "linux": {
      "target": [
        "deb"
      ]
    },
    "win": {
      "target": "NSIS"
    }
  },
  "repository": "https://github.com/vpics/react-electron-updator",  
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.2.1"
  }
}
